<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="librarySystem.dao.StudentAndPermissionDao">


    <select id="findBySno" resultType="librarySystem.domain.Student_Permission">
        SELECT *
        FROM student_permission
        WHERE sno = #{sno}
    </select>
</mapper>